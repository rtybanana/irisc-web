    "core-js": "^3.6.5",
    "lodash.clonedeep": "^4.5.0",
    "module-alias": "^2.2.2",
    "printf": "^0.6.1",
    "prismjs": "^1.29.0",
    "textarea-caret": "^3.1.0",
    "vue": "^2.6.11",
    "vue-prism-editor": "^1.2.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.17.12",
